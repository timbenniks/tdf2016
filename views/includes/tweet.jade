blockquote
  p.entry!=tweet.text
  if typeof( tweet.extended_entities ) !== "undefined"
    for entity in tweet.extended_entities

      if entity[ 0 ].type === 'video'
        figure.media
          .media-wrapper.video
            video( src=entity[ 0 ].video_info.variants[ 0 ].url, autoplay, controls )

      if entity[ 0 ].type === 'photo'
        figure.media
          img( src=entity[ 0 ].media_url )
  
  if typeof( tweet.entities.urls ) !== "undefined"
    for url in tweet.entities.urls
      if url.expanded_url.indexOf( 'vine' ) !== -1
        figure.media
          .media-wrapper.iframe
            iframe( src=url.expanded_url + '/embed/simple?audio=0', frameborder="0" )
  
  p.meta!=tweet.created_at